#!/bin/bash

echo "installing anyenv environments"

~/.anyenv/bin/anyenv install goenv --skip-existing
~/.anyenv/bin/anyenv install nodenv --skip-existing
~/.anyenv/bin/anyenv install pyenv --skip-existing
~/.anyenv/bin/anyenv install rbenv --skip-existing

cat << END
**************************************************
ANYENV INSTALLED!
Please reload your profile (exec $SHELL -l) or open a new session.
bye.
**************************************************
END
